<%= form_for @playdate do  | f | %>
<%= f.label :person_id %>
<br/>
<%= f.collection_select :person_id, Person.all, :id, :name  %>
<br/>
<%= f.label :date%>
<br/>
<%= f.text_field :date%>
<br/>
<%= f.label :time%>
<br/>
<%= f.number_field :time%>
<br/>
<%= f.label :kitten_id%>
<br/>
<%= f.collection_select :kitten_id, Kitten.all, :id, :name%>
<br/>
<%= f.submit %>
<% end %>